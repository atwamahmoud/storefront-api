{
  "name": "storefront-api",
  "version": "1.0.0",
  "description": "An API for building online stores",
  "main": "index.js",
  "author": "Mahmoud Atwa <mahmoudatwa4@outlook.com>",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "jasmine": "jasmine",
    "db:run-server": "docker-compose --env-file .env up -d",
    "db:close-server": "docker-compose --env-file .env down",
    "dev": "npm run db:run-server && nodemon -L",
    "test": "bash ./test.sh",
    "lint": "eslint . --ext ts",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier . --write",
    "start": "npm run db:run-server && npm run db:up && node build/index",
    "db-migrate": "db-migrate",
    "db:up": "db-migrate up",
    "db:down": "db-migrate down",
    "db:reset": "db-migrate reset",
    "apidoc": "apidoc-markdown -i src -o apidoc.md",
    "db:drop": "db-migrate db:drop storefront_api"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.30",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.2",
    "@types/node": "^16.11.10",
    "@types/node-fetch": "2",
    "@types/pg": "^8.6.1",
    "@types/sharp": "^0.29.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "apidoc-markdown": "^7.0.1",
    "cheerio": "^1.0.0-rc.10",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-import-path": "^0.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/jsonwebtoken": "^8.5.6",
    "bcrypt": "^5.0.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jasmine": "^3.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "2",
    "pg": "^8.7.1",
    "sharp": "^0.29.3",
    "supertest": "^6.1.6",
    "validate-color": "^2.2.1"
  }
}
